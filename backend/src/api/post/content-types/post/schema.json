{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "post",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "string"
    },
    "short": {
      "type": "customField",
      "options": {
        "preset": "blockBaloon"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "more": {
      "type": "customField",
      "options": {
        "preset": "toolbarBaloon"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "image": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "posts"
    }
  }
}
